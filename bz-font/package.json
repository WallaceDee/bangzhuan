{
  "name": "bz-font",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0 --copy --mode",
    "build": "vue-cli-service build --mode",
    "lint": "vue-cli-service lint",
    "buildMain": "npm run build --production main",
    "buildManage": "npm run build --production manage",
    "mainServe": "npm run serve main",
    "manageServe": "npm run serve manage"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.6.4",
    "js-md5": "^0.7.3",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "swiper": "^5.3.6",
    "view-design": "^4.1.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.8.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "compression-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "iview-loader": "^1.2.2",
    "style-resources-loader": "^1.3.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "standard"
    ],
    "rules": {
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ],
      "vue/html-indent": "off",
      "no-var": "error",
      "generator-star-spacing": "off",
      "spaced-comment": "off",
      "indent": "off",
      "eol-last": "off",
      "space-before-function-paren": "off",
      "comma-spacing": "off",
      "key-spacing": "off",
      "space-before-blocks": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-tabs": "off",
      "space-infix-ops": "off",
      "no-undef": "off",
      "no-useless-escape": "off",
      "handle-callback-err": "off",
      "keyword-spacing": "off",
      "no-new": "off",
      "no-extend-native": "off",
      "no-mixed-operators": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
